{
  "name": "twitter-clone",
  "version": "1.0.0",
  "description": "A twitter clone, for learning purposes.",
  "main": "index.js",
  "scripts": {
    "install-deps": "cd client && npm install && cd ../server && npm install ",
    "client": "cd client && npm start",
    "server": "cd server && npm run dev",
    "docker-build": "docker-compose build",
    "docker-start": "docker-compose up -d",
    "docker-stop": "docker-compose down",
    "docker-restart": "docker-compose restart",
    "docker-server-logs": "docker-compose logs node_app -f",
    "docker-client-logs": "docker-compose logs twitter_react_app -f"
  },
  "author": "Stephen Park",
  "license": "ISC",
  "dependencies": {
    "postcss-flexbugs-fixes": "^5.0.2"
  }
}
